%A1 function_plus( %A1 a, %A2 b )	{return a + b;}
%A1 function_minus( %A1 a, %A2 b )	{return a - b;}
%A1 function_mul( %A1 a, %A2 b )	{return a * b;}
%A1 function_div( %A1 a, %A2 b )	{return a / b;}
bool function_gt( %A1 a, %A2 b )	{return a > b;}
bool function_ge( %A1 a, %A2 b )	{return a >= b;}
bool function_lt( %A1 a, %A2 b )	{return a < b;}
bool function_le( %A1 a, %A2 b )	{return a <= b;}
bool function_eq( %A1 a, %A2 b )	{return a == b;}
bool function_ne( %A1 a, %A2 b )	{return a != b;}
%A1 function_assign( %A1 a, %A2 b )	{return a = b;}

